{
    "name": "dragmerge/dragmerge",
    "description": "PDF data merge tool with drag-and-drop layout editor",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "tecnickcom/tcpdf": "^6.7",
        "setasign/fpdi-tcpdf": "^2.3",
        "league/csv": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "DragMerge\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
